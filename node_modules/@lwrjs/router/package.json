{
    "name": "@lwrjs/router",
    "description": "LWR Client-side Navigation APIs and services",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "version": "0.10.2",
    "homepage": "https://developer.salesforce.com/docs/platform/lwr/overview",
    "repository": {
        "type": "git",
        "url": "https://github.com/salesforce-experience-platform-emu/lwr.git",
        "directory": "packages/@lwrjs/router"
    },
    "bugs": {
        "url": "https://github.com/salesforce-experience-platform-emu/lwr/issues"
    },
    "type": "module",
    "types": "build/es/services/index.d.ts",
    "main": "build/cjs/services/index.cjs",
    "module": "build/es/services/index.js",
    "exports": {
        ".": {
            "import": "./build/es/services/index.js",
            "require": "./build/cjs/services/index.cjs"
        },
        "./module-provider": {
            "import": "./build/es/services/module-provider/index.js",
            "require": "./build/cjs/services/module-provider/index.cjs"
        },
        "./pageObjects/*": {
            "import": "./pageObjects/*.js",
            "require": "./pageObjects/*.cjs"
        }
    },
    "files": [
        "build/**/*.js",
        "build/**/*.cjs",
        "build/**/*.d.ts",
        "build/**/*.js-meta.xml",
        "build/**/*.css",
        "build/**/*.html",
        "pageObjects"
    ],
    "scripts": {
        "build:ts": "tsc -b",
        "build": "yarn build:ts && node ../../../bin/pack-lwc --dir src/modules build/modules && yarn utam:compile && yarn build:bundle",
        "build:bundle": "rollup --config scripts/rollup.moduleBundle.config.cjs",
        "utam:compile": "utam -c utam.config.cjs",
        "clean": "rm -rf build",
        "test": "yarn test:client && yarn test:server",
        "test:client": "jest --config ./jest.config.cjs",
        "test:server": "jest --config ./jest.server.config.cjs"
    },
    "dependencies": {
        "@lwrjs/client-modules": "0.10.2",
        "@lwrjs/diagnostics": "0.10.2",
        "@lwrjs/shared-utils": "0.10.2",
        "ajv": "6.12.6"
    },
    "devDependencies": {
        "@lwc/jest-preset": "^11.8.0",
        "@rollup/plugin-typescript": "^11.1.0",
        "jest": "^26.6.3",
        "rollup": "^2.78.0",
        "ts-jest": "^26.5.6",
        "utam": "^2.0.2"
    },
    "peerDependencies": {
        "lwc": "2.x || 3.x"
    },
    "lwc": {
        "modules": [
            {
                "dir": "build/modules"
            },
            {
                "name": "lightning/navigation",
                "path": "build/modules/lwr/navigation/navigation.js"
            }
        ],
        "expose": [
            "lwr/routerContainer",
            "lwr/navigation",
            "lightning/navigation",
            "lwr/outlet",
            "lwr/router",
            "lwr/routerErrors",
            "lwr/routerBridge"
        ]
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "volta": {
        "extends": "../../../package.json"
    },
    "gitHead": "40988dfc093a3b9dea859f1bf61a49bca1edb0c5"
}
